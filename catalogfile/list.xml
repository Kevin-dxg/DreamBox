<dbl>
    <meta />
    <render>
        <list backgroundColor="#EEEEEE" bottomToBottom="parent" id="stud_list" leftToLeft="parent" marginBottom="0dp"
            marginTop="0dp" orientation="vertical" rightToRight="parent" src="${ext.db_list}" topToTop="parent"
            width="fill" height="fill">
            <header>
                <text gravity="center" height="60dp" leftToLeft="parent" rightToRight="parent" src="I'm header View"
                    width="fill" />
            </header>
            <vh>
                <view backgroundColor="#666666" height="1px" topToTop="parent" width="fill" />
                <image bottomToBottom="parent" height="40dp" id="img" leftToLeft="parent" marginBottom="15dp"
                    marginLeft="20dp" marginTop="15dp" src="${img}" topToTop="parent" width="40dp" />
                <text style="bold" color="#333333" id="title" leftToRight="img" marginLeft="12dp" marginTop="-2dp"
                    size="16dp" src="${title}" topToTop="img" />
                <text color="#999999" id="sub_title" leftToRight="img" marginLeft="12dp" marginTop="8dp" size="12dp"
                    src="${subtitle}" topToBottom="title" />
                <image bottomToBottom="parent" height="wrap" id="arrow" marginRight="30dp" rightToRight="parent"
                    src="${arrow}" topToTop="parent" width="10dp" />
            </vh>
            <footer>
                <view backgroundColor="#666666" height="1px" id="line" topToTop="parent" width="fill" />
                <text gravity="center" height="60dp" leftToLeft="parent" rightToRight="parent" src="I'm footer View"
                    topToBottom="line" width="fill" />
            </footer>
        </list>
    </render>
</dbl>
